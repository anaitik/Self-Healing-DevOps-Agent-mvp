# Gemini (required for AI diagnosis)
GEMINI_API_KEY=your_gemini_api_key

# Grafana Loki (optional – for Load from Loki and webhook)
LOKI_URL=http://localhost:3100
GRAFANA_LOKI_USER=
GRAFANA_LOKI_API_TOKEN=

# GitHub (backend only – for creating patch PRs)
